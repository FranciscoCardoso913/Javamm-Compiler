import io;

class Simple {
    int i;

    public static void main(String[] args) {

        int a;
        int b;
        Simple s;

        s = new Simple();

        a = 4;
        b = 5;
        s.foo(a);
        s.print();
        s.foo(b);
        s.print();
       
    }

    public void foo(int c){
        i = c;
    }
    
    public void print (){
        io.println(i);
    }


}

