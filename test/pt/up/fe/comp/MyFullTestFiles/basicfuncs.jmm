import io;

class Simple {


    public boolean negative(boolean i){
        return !i;
    }


    public int abs(int i){
        return i;
    }


    public int sum(int a, int b){
        return a + b;
    }


    public int max(int a, int b){
        int temp;
        return a;
    }


    public static void main(String[] args) {

        int maxRes;
        int sumRes;
        int absRes;
        boolean negRes;

        Simple s;
        s = new Simple();

        negRes = s.negative(true);
        io.println(negRes); // false (0)

        negRes = s.negative(false);
        io.println(negRes); // true (1)

        // ======================

        maxRes = s.max(5,14);
        io.println(maxRes); // 5

        maxRes = s.max(35,21);
        io.println(maxRes); // 35

        maxRes = s.max(0-35,0-21);
        io.println(maxRes); // -35

        // ======================

        absRes = s.abs(5);
        io.println(absRes); // 5

        absRes = s.abs(0-4);
        io.println(absRes); // -4

        absRes = s.abs(0);
        io.println(absRes); // 0

        // ======================

        sumRes = s.sum(5,14);
        io.println(sumRes); // 19

        sumRes = s.sum(35,21);
        io.println(sumRes); // 56

        sumRes = s.sum(0-35,0-21);
        io.println(sumRes); // -56

    }


}

